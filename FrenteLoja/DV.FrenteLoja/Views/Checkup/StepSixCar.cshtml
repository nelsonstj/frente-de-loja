        @Html.HiddenFor(a => a.Id)
        @Html.HiddenFor(a => a.CheckupId)
        @Html.HiddenFor(a => a.OrcamentoId)


        <div>
            <div class="header">
                <label> ACESSÓRIOS </label>
            </div>
        </div>

        <form method="post">

            <div class="col-md-12 checkbox">
                @Html.CheckBoxList(model => model.Palhetas, model => model.PalhetasExecutado)
            </div>

            <div class="col-md-12 checkbox">
                @Html.CheckBoxList(model => model.Calotas, model => model.CalotasExecutado)
            </div>

            <div class="col-md-12 checkbox">
                @Html.CheckBoxList(model => model.Extintor, model => model.ExtintorExecutado)
            </div>

            <div class="col-md-12 checkbox">
                @Html.CheckBoxList(model => model.InjecaoEletronica, model => model.InjecaoEletronicaExecutado)
            </div>

            <div class="col-md-12 checkbox">
                @Html.CheckBoxList(model => model.HigienizacaoDeAr, model => model.HigienizacaoDeArExecutado)
            </div>

            <div class="col-md-12 checkbox last">
                @Html.CheckBoxList(model => model.FiltroArCondicionado, model => model.FiltroArCondicionadoExecutado)
            </div>


            <br />
            <div class="row">
                <div class="col-md-6">
                    <div class="col-md-12 noPadding paddingCampos noMargin">
                        <button type="reset" class="btn btn-outline-secondary  btn-block" onclick="window.history.back()">Voltar</button>
                    </div>
                </div>

                <div class="col-md-6">
                    <div class="col-md-12 noPadding paddingCampos noMargin">
                        <input type="submit" value="Prosseguir" class="btn btn-block blue" />
                    </div>
                </div>
            </div>
        </form>
