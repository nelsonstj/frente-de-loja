    <div>
        <div class="col-md-4">
            <h3> CHECK-UP </h3>
        </div>
        <div class="col-md-12 text-align-end">
            @Html.StepControl(6, 8)
        </div>
    </div>
</header>



<main>
    @using (@Html.BeginForm("StepSevenCar", "Checkup", FormMethod.Post))
